{
  "name": "@ekkolyth/ui",
  "version": "0.1.17",
  "description": "Shared UI Library for my personal projects",
  "keywords": [
    "theme",
    "dark-mode",
    "next-themes",
    "tanstack-router",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekkolyth/ui.git"
  },
  "license": "MIT",
  "author": "ekkolyth",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "storybook dev -p 3000 --no-open",
    "build": "tsup",
    "build:clean": "rm -rf dist && npm run build",
    "start": "next start",
    "lint": "biome lint .",
    "format": "biome check --write .",
    "format:check": "biome check .",
    "build-storybook": "storybook build",
    "ui-update": "node scripts/shadcn-update.js add",
    "pub": "node scripts/publish.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js"
    },
    "./tanstack": {
      "types": "./dist/tanstack.d.ts",
      "import": "./dist/tanstack.js"
    },
    "./themes": {
      "style": "./dist/themes.css",
      "default": "./dist/themes.css"
    },
    "./themes/theme-toggle": {
      "types": "./dist/themes/theme-toggle.d.ts",
      "import": "./dist/themes/theme-toggle.js"
    },
    "./themes/theme-select": {
      "types": "./dist/themes/theme-select.d.ts",
      "import": "./dist/themes/theme-select.js"
    },
    "./catpuccin/latte": {
      "style": "./dist/catpuccin/latte.css",
      "default": "./dist/catpuccin/latte.css"
    },
    "./catpuccin/frappe": {
      "style": "./dist/catpuccin/frappe.css",
      "default": "./dist/catpuccin/frappe.css"
    },
    "./catpuccin/macchiato": {
      "style": "./dist/catpuccin/macchiato.css",
      "default": "./dist/catpuccin/macchiato.css"
    },
    "./catpuccin/mocha": {
      "style": "./dist/catpuccin/mocha.css",
      "default": "./dist/catpuccin/mocha.css"
    },
    "./ekko-os": {
      "style": "./dist/ekko-os.css",
      "default": "./dist/ekko-os.css"
    },
    "./tokyo-night": {
      "style": "./dist/tokyo-night.css",
      "default": "./dist/tokyo-night.css"
    },
    "./ekkolyth": {
      "style": "./dist/ekkolyth.css",
      "default": "./dist/ekkolyth.css"
    },
    "./ekko-playlist": {
      "style": "./dist/ekko-playlist.css",
      "default": "./dist/ekko-playlist.css"
    }
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.562.0",
    "next": "16.1.2",
    "radix-ui": "^1.4.3",
    "react-day-picker": "^9.13.0",
    "react-resizable-panels": "^4.4.1",
    "recharts": "2.15.4",
    "shadcn": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@chromatic-com/storybook": "^5.0.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-themes": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/nextjs-vite": "^10.1.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-router": "^1.150.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.30",
    "@types/postcss-import": "^14.0.3",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17",
    "@vueless/storybook-dark-mode": "^10.0.6",
    "jsdom": "^27.4.0",
    "next-themes": "^0.4.6",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "storybook": "^10.1.11",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "next-themes": "^0.4.6",
    "@tanstack/react-router": "^1.150.0"
  },
  "peerDependenciesMeta": {
    "next-themes": {
      "optional": true
    },
    "@tanstack/react-router": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/index.js",
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "sharp",
    "unrs-resolver"
  ]
}
